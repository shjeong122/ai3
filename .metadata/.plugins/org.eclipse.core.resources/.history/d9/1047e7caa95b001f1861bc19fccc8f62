package P489;

import java.io.FileWriter;

public class FileWriterEx {

	public static void main(String[] args) {
		FileWriter fw = null;
		try {
			fw = new FileWriter("file.txt");
			fw.write("Java");
		} catch(Exception e) {
			e.printStackTrace();
		}finally {
			try {
				fw.close();
			}catch(Exception e) {
				e.printStackTrace();
			}
		}

	}

}
