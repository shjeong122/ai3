package ch07_02;

public class ColorPoint extends Point {

	String color;
	
	Point p = new Point();
	
	
	void colorPoint() {
		System.out.print("colorPoint : " + getColor() + "("+p.x +", "+p.y+")");
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}
	


}
