package P482;

public class AccountMain {

	public static void main(String[] args) throws InsufficientEx {
		Account account = new Account();
		try {
			account.deposit(10000); //예금
			System.out.println("예금액 : " + account.getBalance());
			account.wihthdraw(25000);
			System.out.println("잔액 : " + account.getBalance());
		} catch(InsufficientEx e) {
			e.printStackTrace();
		}catch(DepositException e1) {
			e1.printStackTrace();
	}
	}
}
