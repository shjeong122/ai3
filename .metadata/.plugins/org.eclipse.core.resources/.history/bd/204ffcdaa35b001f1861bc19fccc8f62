package P488;

public class LoginEx {

	public static void main(String[] args) {
		try {
			login("White", "12345");
		} catch(Exception e) {
			System.out.println(e.getMessage()));
			System.out.println(e.getLocalizedMessage());
		}
		try {
			login("blue", "54321");	
		} catch(Exception e) {
			System.out.println(e.getMessage()));
			System.out.println(e.getLocalizedMessage());
		
		
	}

}
