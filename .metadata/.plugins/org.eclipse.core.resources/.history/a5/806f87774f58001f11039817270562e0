package ch07_03;

public class ComputerEx {

	public static void main(String[] args) {
		int r=10;
		//부모 객체 생성
		Calculator c = new Calculator();
		System.out.println("원면적 : " + c.areaCircle(r));
		System.out.println();
		//자식 객체 생성
		Computer co = new Computer();
		System.out.println("원면적 : " + co.areaCircle(r));
		System.out.println();
		
	}	

}
